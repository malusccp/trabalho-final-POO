{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",

  
    "target": "es2016",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    

    "esModuleInterop": true,


    "strict": true,
    "skipLibCheck": true,

    "ignoreDeprecations": "5.0"
  },
  "include": ["src/**/*"]
}